@model List <PhoneAPP.Models.PhoneBook>

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
<br />
<h3>User list</h3>
<th>
<table id="tableOrder" border="1" cellpadding="2">

    <tr><th>ID</th><th>Name</th><th>Surname</th><th>PID</th><th>City</th><th>Postal Code</th><th>Street</th><th>Number</th><th>Phone number</th></tr>
    @foreach (var items in Model)
    {

        <tr><th>@items.clientID</th><td>@items.clientName</td><td>@items.clientSurname</td><td>@items.clientPID</td><td>@items.clientCity</td><td>@items.clientPostalCode</td><td>@items.clientStreet</td><td>@items.clientNumber</td><td>@items.clientPhoneNumber</td></tr>

    }

</table>
</th>